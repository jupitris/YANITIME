<h1>Listing users</h1>

<table>
  <tr>
    <th>Id</th>
    <th>User</th>
    <th>Password</th>
    <th>Name</th>
    <th>Created by</th>
    <th>Created on</th>
    <th>Updated by</th>
    <th>Updated on</th>
    <th>Delete no</th>
    <th>Version no</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.user_id %></td>
    <td><%= user.password %></td>
    <td><%= user.name %></td>
    <td><%= user.created_by %></td>
    <td><%= user.created_on %></td>
    <td><%= user.updated_by %></td>
    <td><%= user.updated_on %></td>
    <td><%= user.delete_no %></td>
    <td><%= user.version_no %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
